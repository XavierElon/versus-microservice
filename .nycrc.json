{
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true,
    "include": [
        "src/**/!(*.test.*).[tj]s?(x)"
    ],
    "exclude": [
        "src/_tests_/**/*.*",
        "src/config/config.ts",
        "src/routes/store.routes.ts",
        "src/structures/*",
        "src/controllers/store.controllers.ts",
        "src/middleware/*.ts"
    ],
    "reporter": [
        "html",
        "lcov",
        "text",
        "text-summary"
    ],
    "report-dir": "coverage",
    "statements": 0,
    "branches": 0,
    "functions": 0,
    "lines": 0
}