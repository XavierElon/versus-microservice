{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint",
        "unused-imports"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        // "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
        "no-console": 1,
        "quotes": [2, "single", { "avoidEscape": true }],
        "no-useless-return": 1,
        "no-unused-vars": 1,
        "unused-imports/no-unused-imports": "warn",
    }
}